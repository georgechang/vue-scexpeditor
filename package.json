{
  "name": "vue-scexpeditor",
  "description": "A Vue plugin to support Vue components in Sitecore Experience Editor.",
  "version": "1.0.3",
  "main": "index.js",
  "keywords": [
    "sitecore",
    "vue",
    "vuejs",
    "experience",
    "editor"
  ],
  "author": "George Chang",
  "scripts": {
    "dev": "webpack -d --watch --progress --colors --config build/webpack.dev.config.js",
    "build": "webpack -p --progress --colors --config build/webpack.prod.config.js"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "vue": "^2.5.17",
    "vue-cli": "^2.9.6",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0",
    "webpack-merge": "^4.1.4"
  }
}
